<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <connectionStrings>
      <add name="TSSDatabase" connectionString="server=.\sqlexpress;database=TechnicalServiceSystem;User ID=TssUser;Password=TssDbTest;max pool size=50;min pool size=5" providerName="System.Data.SqlClient" />
    </connectionStrings>
  <appSettings>
